<section>
  <h1>People Found</h1>
  <h2>{{personName}}</h2>
  <main>
  {{#if flag}}
  <ul>
  {{#each people}}  
    <article>
      <li>
          <a href="/details/{{id}}"> {{this.firstName}} {{this.lastName}}</a>
      </li>
    </article>
  {{/each}}
  </ul>
  {{else}}
  <p class="not-found">We're sorry, but no results were found for {{personName}}</p>
  {{/if}}
  <a href = "/">Make another search</a>
  </main>
</section>